SELECT *
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;

SELECT MIN (TRANSACTION_TIME)
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;

SELECT MAX (TRANSACTION_TIME)
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;

SELECT STORE_ID,
       STORE_LOCATION,
       CASE 
         WHEN TRANSACTION_TIME BETWEEN '06:00:00' AND '09:59:00' THEN 'EARLY_RUSH'
         WHEN TRANSACTION_TIME BETWEEN '10:00:00' AND '12:59:00' THEN 'MORNING'
         WHEN TRANSACTION_TIME BETWEEN '13:00:00' AND '15:59:00' THEN 'MID-DAY'
         WHEN TRANSACTION_TIME BETWEEN '16:00:00' AND '17:59:00' THEN 'AFTERNOON'
         WHEN TRANSACTION_TIME  > '17:59:00' THEN 'EVENING'
         END AS  TIME_OF_THE_DAY
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;


SELECT DISTINCT COUNT (TRANSACTION_ID) AS TOTAL_CUSTOMERS
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;

SELECT DISTINCT PRODUCT_CATEGORY,
                COUNT (PRODUCT_TYPE) AS DIFFERENT_PRODUCTS
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;
GROUP BY ALL

SELECT MAX (TRANSACTION_DATE) AS LAST_DATA
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;

SELECT MIN (TRANSACTION_DATE) AS LAST_DATA
FROM COFFEE_SALES_DATABASE.PUBLIC.COFFEESALES;
